            "id": "pickxspeed",
            "speaker": "npc",
            "lines": [
                "You'd like to purchase an X Speed for $250?"
            ],
            "input": {
                "type": "option",
                "vertical": false,
                "options": [
                    {
                        "isSelectable": "q.player.has_tag('MoneyHave250')",
                        "text": "Buy x1",
                        "value": "buy1",
                        "action": [
                            "q.run_command('execute as ' + q.player.username + ' run give @s cobblemon:x_speed 1');",
                            "q.run_command('execute as ' + q.player.username + ' run playsound minecraft:entity.experience_orb.pickup ambient @s ~ ~ ~ 1 1 1');",
