                "vertical": false,
                "options": [
                    {
                        "text": "Buy",
                        "value": "buy",
                        "action": "q.dialogue.set_page('buy1');"
                    },
                    {
                        "text": "No Thanks",
                        "value": "nothanks",
                        "action": "q.dialogue.set_page('end');"
                    }
                ]
            }
        },
        {
            "id": "buy1",
            "speaker": "npc",
            "lines": [
