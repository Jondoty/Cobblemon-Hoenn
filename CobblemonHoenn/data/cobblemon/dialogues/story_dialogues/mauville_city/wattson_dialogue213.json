            "input": {
                "type": "option",
                "vertical": false,
                "options": [
                    {
                        "text": "Next",
                        "value": "next",
                        "action": "q.dialogue.set_page('page13');"
                    }
                ]
            }
        },
        {
            "id": "page13",
            "speaker": "wattson",
            "lines": [
                "Now if you go to New Mauville, the doors will open automatically!"
            ],
            "input": {
                "type": "option",
                "vertical": false,
                "options": [
                    {
                        "text": "Next",
