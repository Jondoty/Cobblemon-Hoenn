            "input": {
                "type": "option",
                "vertical": false,
                "options": [
                    {
                        "text": "Next",
                        "value": "next",
                        "action": "q.dialogue.set_page('page15');"
                    }
                ]
            }
        },
        {
            "id": "page15",
            "speaker": "wattson",
            "lines": [
                "When you get inside, go press the button on the computer in the back."
